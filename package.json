    "babel-eslint": "^4.1.6",
    "babel-loader": "*",
    "babel-preset-es2015": "^6.2.4",
    "eslint": "^1.10.2",
    "eslint-loader": "^1.1.1",
    "eslint-plugin-angular": "^0.14.0",
    "eslint-plugin-jasmine": "^1.6.0",
    "file-loader": "0.8.4",
